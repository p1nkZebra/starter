{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "angular-cli-project": {
            "root": "",
            "sourceRoot": "src/main/angular",
            "projectType": "application",
            "prefix": "app",
            "schematics": {
                "@schematics/angular:component": {
                    "styleext": "less"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "target/classes/static",
                        "index": "src/main/angular/index.html",
                        "main": "src/main/angular/main.ts",
                        "polyfills": "src/main/angular/polyfills.ts",
                        "tsConfig": "src/main/angular/tsconfig.app.json",
                        "assets": [
                            "src/main/angular/favicon.ico",
                            "src/main/angular/assets",
                            "src/main/angular/images"
                        ],
                        "statsJson": true,
                        "styles": [
                            "node_modules/@clr/icons/clr-icons.min.css",
                            "node_modules/@clr/ui/clr-ui.min.css",
                            "src/main/angular/styles.css"
                        ],
                        "scripts": [
                            "node_modules/mutationobserver-shim/dist/mutationobserver.min.js",
                            "node_modules/@webcomponents/custom-elements/custom-elements.min.js",
                            "node_modules/@clr/icons/clr-icons.min.js"
                        ]
                    },
                    "configurations": {
                        "hmr": {
                            "fileReplacements": [
                                {
                                    "src": "src/main/angular/environments/environment.ts",
                                    "replaceWith": "src/main/angular/environments/environment.hmr.ts"
                                }
                            ]
                        },
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/main/angular/environments/environment.ts",
                                    "with": "src/main/angular/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "aot": true,
                            "extractCss": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "angular-cli-project:build"
                    },
                    "configurations": {
                        "hmr": {
                            "hmr": true,
                            "browserTarget": "angular-cli-project:build:hmr"
                        },
                        "production": {
                            "browserTarget": "angular-cli-project:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "angular-cli-project:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/main/angular/test.ts",
                        "polyfills": "src/main/angular/polyfills.ts",
                        "tsConfig": "src/main/angular/tsconfig.spec.json",
                        "karmaConfig": "src/main/angular/karma.conf.js",
                        "scripts": [],
                        "assets": [
                            "src/main/angular/favicon.ico",
                            "src/main/angular/assets",
                            "src/main/angular/images"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "angular-cli-project-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "angular-cli-project:serve"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "angular-cli-project"
}
